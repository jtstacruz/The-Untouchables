#PATH TO DATABASE USING COMMAND PROMPT
	cd c:\\xampp\mysql\\bin
	mysql.exe -u root

#create database named "Reviewer" in XAMPP
	use reviewer

SQL SCRIPT

CREATE TABLE REVIEWSITES(REVIEWSITES_ID INTEGER PRIMARY KEY NOT NULL, REVIEWSITES_NAME VARCHAR(25) NOT NULL);

INSERT INTO REVIEWSITES VALUES(001, 'Agoda');
INSERT INTO REVIEWSITES VALUES(002, 'Booking');
INSERT INTO REVIEWSITES VALUES(003, 'TripAdvisor');

CREATE TABLE CUSTOMER (CSTMR_ID INTEGER NOT NULL PRIMARY KEY AUTO_INCREMENT, REVIEWSITES_ID INTEGER NOT NULL, CSTMR_REVIEW VARCHAR(5000), CSTMR_RATINGDATE VARCHAR(100), CSTMR_RATING VARCHAR(10), FOREIGN KEY(REVIEWSITES_ID) REFERENCES REVIEWSITES(REVIEWSITES_ID));

CREATE TABLE PREPROCESSED(PREPROCESSED_ID INTEGER PRIMARY KEY NOT NULL AUTO_INCREMENT, PREPROCESSED_REVIEW VARCHAR(5000) NOT NULL);

select *from reviewsites;
select *from customer;
describe reviewsites;
describe customer;


