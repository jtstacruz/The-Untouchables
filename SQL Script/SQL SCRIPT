PATH TO DATABASE USING COMMAND PROMPT

cd c:\\xampp\mysql\\bin
mysql.exe -u root
use reviewer
select *from reviewsites;
select *from customer;
describe reviewsites;
describe customer;

SQL SCRIPT

CREATE TABLE REVIEWSITES(REVIEWSITES_ID INTEGER NOT NULL, REVIEWSITES_NAME VARCHAR(25) NOT NULL, CONSTRAINT REVIEWSITES_PK PRIMARY KEY (REVIEWSITES_ID));

INSERT INTO REVIEWSITES VALUES(001, 'Agoda');
INSERT INTO REVIEWSITES VALUES(002, 'Booking');
INSERT INTO REVIEWSITES VALUES(003, 'TripAdvisor');

CREATE TABLE CUSTOMER (CSTMR_ID INTEGER NOT NULL PRIMARY KEY, REVIEWSITES_ID INTEGER NOT NULL, CSTMR_REVIEWER VARCHAR(25), CSTMR_REVIEWTITLE VARCHAR(100), CSTMR_REVIEW VARCHAR(5000), CSTMR_RATINGDATE VARCHAR(100), CSTMR_RATING VARCHAR(10), FOREIGN KEY(REVIEWSITES_ID) REFERENCES REVIEWSITES(REVIEWSITES_ID));



select *from reviewsites;
select *from customer;
describe reviewsites;
describe customer;

